{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "crons": [
        {
            "path": "/crons/daily",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/crons/weekly",
            "schedule": "0 0 * * 0"
        }
    ]
}